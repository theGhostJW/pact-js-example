{
  "consumer": {
    "name": "dog-consumer"
  },
  "provider": {
    "name": "dog-provider"
  },
  "interactions": [
    {
      "description": "a request for a single dog",
      "providerState": "i have a list of dogs",
      "request": {
        "method": "GET",
        "path": "/dogs/1",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": [
          {
            "dog": 1
          }
        ]
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}